}
\examples{
match.call.string()
yayandwow <- function()
    match.call.string()

yayandwow()

#########################

yay <- function(...) {
	wow <- function(...) match.call.string(...)
	wow(...)
}

yay()
yay(n=1)
yay(n=2)
yay(n=3)

}
